{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["es2019", "dom"],
    "types": ["cypress", "node", "mocha"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "allowJs": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["cypress/**/*.ts"]
} 